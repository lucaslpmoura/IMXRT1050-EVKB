  <example id="evkmimxrt1050_cmsis_lpspi_int_b2b_transfer_slave" name="cmsis_lpspi_int_b2b_transfer_slave" category="cmsis_driver_examples/lpspi" dependency="platform.drivers.clock.MIMXRT1052 platform.drivers.common.MIMXRT1052 platform.devices.MIMXRT1052_CMSIS.MIMXRT1052 platform.devices.MIMXRT1052_startup.MIMXRT1052 platform.drivers.lpuart.MIMXRT1052 platform.utilities.debug_console.MIMXRT1052 platform.drivers.iomuxc.MIMXRT1052 platform.utilities.assert.MIMXRT1052 platform.drivers.igpio.MIMXRT1052 platform.drivers.edma.MIMXRT1052 platform.drivers.dmamux.MIMXRT1052 platform.drivers.lpspi.MIMXRT1052 platform.drivers.lpspi_edma.MIMXRT1052 platform.drivers.lpspi_cmsis.MIMXRT1052 platform.CMSIS_Driver_Include.SPI.MIMXRT1052 platform.CMSIS_Driver_Include.Common.MIMXRT1052" toolchain="iar mdk armgcc">
    <projects>
      <project type="com.crt.advproject.projecttype.exe" nature="org.eclipse.cdt.core.cnature"/>
    </projects>
    <toolchainSettings>
      <!-- Curently, mcux just supports one common settings set for both debug and release -->
      <toolchainSetting id_refs="com.nxp.mcuxpresso">
        <!-- asm compiler -->
        <!-- gcc compiler -->
        <option id="com.crt.advproject.c.misc.dialect" type="enum">
          <value>com.crt.advproject.misc.dialect.gnu99</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.debug.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.release.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="gnu.c.compiler.option.preprocessor.def.symbols" type="stringList">
          <value>CPU_MIMXRT1052DVL6A</value>
        </option>
        <option id="gnu.c.compiler.option.optimization.flags" type="string">
          <value>-fno-common</value>
        </option>
        <option id="gnu.c.compiler.option.misc.other" type="string">
          <value>-c  -ffunction-sections  -fdata-sections  -ffreestanding  -fno-builtin  -mfpu=fpv5-d16</value>
        </option>
        <!-- gcc linker -->
        <option id="gnu.c.link.option.nostdlibs" type="boolean">
          <value>true</value>
        </option>
        <option id="gnu.c.link.option.other" type="stringList">
          <value>--gc-sections</value>
        </option>
      </toolchainSetting>
    </toolchainSettings>
